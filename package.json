{
    "name": "mobile-food-tracker",
    "version": "1.0.0",
    "description": "Tracking service for mobile food facilities based in San Francisco.",
    "author": "Chase Sigmon",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier src/ test/ --check",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:prod": "nest build && node dist/main",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "test": "jest --runInBand",
        "test:watch": "jest --watch --maxWorkers=25%",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
        "docker:build": "docker build -t mobile-food-tracker .",
        "docker:start": "docker run -p 3000:3000 mobile-food-tracker",
        "docker:stop": "docker stop $(docker ps -q --filter ancestor=mobile-food-tracker )"
    },
    "dependencies": {
        "@nestjs/cli": "^10.0.4",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/swagger": "^7.0.3",
        "axios": "^1.4.0",
        "csvtojson": "^2.0.10",
        "express": "^4.18.2",
        "nestjs-redoc": "^2.2.2",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/sinon": "^10.0.16",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    }
}
